<?xml version="1.0" ?>
<robot name="kr20" xmlns:xacro="http://www.ros.org/wiki/xacro">
  
  <!-- BEGIN - Include -->
  <xacro:include filename="$(find kuka_kr20_description)/urdf/robot_parameters.xacro" />
