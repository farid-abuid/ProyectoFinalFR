    <color rgba="0 0.36 0.012 1.0"/>
  </material>
  
  <material name="LightGreen">
    <color rgba="0.82 0.95 0.82 1.0"/>
  </material>  
  
  <material name="White">
    <color rgba="0.96 0.96 0.96 1.0"/>
  </material>
  
  <material name="Grey">
    <color rgba="0.56 0.56 0.56 1.0"/>
